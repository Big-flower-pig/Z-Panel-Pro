{
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.encoding": "utf8",
  "files.autoGuessEncoding": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "[shellscript]": {
    "files.eol": "\n",
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[bash]": {
    "files.eol": "\n"
  }
}